public class minimum {
    public static void main(String args[]){
        int a[]={42,56,-11,68,-52,0,53,64};
        int min = a[0];
        for(int i=0 ; i<= a.length-1 ; i++){
            if(min > a[i]){
                min = a[i];
            }
        }
        System.out.println(min);
    }
}
